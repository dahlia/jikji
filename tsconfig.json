{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2015",
    "module": "ES2020",
    "moduleResolution": "classic",
    "jsx": "react",
    "lib": ["ES2020", "DOM"],
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "typedocOptions": {
    "exclude": ["*_test.ts", "**/*_test.ts"],
    "entryPoints": ["*.ts", "**/*.ts"]
  }
}
